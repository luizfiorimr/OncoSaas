{
  "name": "medsaas-oncologia",
  "version": "0.1.0",
  "private": true,
  "description": "Plataforma de Otimização de Processos Oncológicos",
  "scripts": {
    "setup": "bash scripts/setup.sh || scripts\\setup.bat",
    "validate": "node scripts/validate-setup.js",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "dev:https": "concurrently \"npm run frontend:dev:https\" \"npm run backend:dev:https\"",
    "dev:all": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\" \"npm run ai:dev\"",
    "build": "npm run backend:build && npm run frontend:build",
    "build:docker": "docker-compose -f docker-compose.yml build",
    "build:docker:prod": "docker-compose -f docker-compose.prod.yml build",
    "start": "concurrently \"npm run frontend:start\" \"npm run backend:start\"",
    "test": "npm run backend:test",
    "test:all": "npm run backend:test",
    "lint": "npm run backend:lint && npm run frontend:lint",
    "lint:check": "npm run backend:lint:check && npm run frontend:lint",
    "lint:fix": "npm run backend:lint && npm run frontend:lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "type-check": "npm run backend:type-check && npm run frontend:type-check",
    "generate-certs": "node scripts/generate-ssl-certs.js",
    "backend:dev": "cd backend && npm run start:dev",
    "backend:dev:https": "cd backend && npm run start:dev:https",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm run start:prod",
    "backend:test": "cd backend && npm run test",
    "backend:test:cov": "cd backend && npm run test:cov",
    "backend:lint": "cd backend && npm run lint",
    "backend:lint:check": "cd backend && npm run lint:check",
    "backend:type-check": "cd backend && npm run type-check",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:dev:https": "cd frontend && npm run dev:https",
    "frontend:build": "cd frontend && npm run build",
    "frontend:start": "cd frontend && npm run start",
    "frontend:lint": "cd frontend && npm run lint",
    "frontend:lint:fix": "cd frontend && npm run lint:fix",
    "frontend:type-check": "cd frontend && npm run type-check",
    "ai:dev": "cd ai-service && source venv/bin/activate && uvicorn src.main:app --reload --host 0.0.0.0 --port 8001",
    "ai:dev:windows": "cd ai-service && venv\\Scripts\\activate && uvicorn src.main:app --reload --host 0.0.0.0 --port 8001",
    "docker:infra": "docker-compose up -d postgres redis rabbitmq",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:backend": "docker-compose logs -f backend",
    "docker:logs:frontend": "docker-compose logs -f frontend",
    "docker:logs:ai": "docker-compose logs -f ai-service",
    "docker:ps": "docker-compose ps",
    "docker:clean": "docker-compose down -v",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:migrate:deploy": "cd backend && npx prisma migrate deploy",
    "db:migrate:reset": "cd backend && npx prisma migrate reset",
    "db:generate": "cd backend && npx prisma generate",
    "db:seed": "cd backend && npx prisma db seed",
    "db:studio": "cd backend && npx prisma studio",
    "db:push": "cd backend && npx prisma db push",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
